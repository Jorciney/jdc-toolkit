{
  "name": "gitlab-improved-mr-comments",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/chrome-extensions/gitlab-improved-mr-comments/src",
  "prefix": "jdc-toolkit",
  "tags": ["scope:npm"],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/chrome-extensions/gitlab-improved-mr-comments/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/chrome-extensions/gitlab-improved-mr-comments/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/chrome-extensions/gitlab-improved-mr-comments/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/chrome-extensions/gitlab-improved-mr-comments/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
